{
	"name": "fxwatch",
	"version": "0.0.1",
	"description": "Financial tool for analytics",
	"main": "app.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"build": "better-npm-run build",
		"buildwatch": "better-npm-run buildwatch",
		"buildinfo": "webpack --progress --colors --profile --display-modules",
		"product": "better-npm-run product"
	},
	"betterScripts": {
		"build": {
			"command": "webpack --progress --colors",
			"env": {
				"NODE_ENV": "development"
			}
		},
		"buildwatch": {
			"command": "webpack --progress --colors --watch",
			"env": {
				"NODE_ENV": "development"
			}
		},
		"product": {
			"command": "webpack --config webpack.cfg.prod.js --progress --colors --profile --display-modules",
			"env": {
				"NODE_ENV": "production"
			}
		}
	},
	"keywords": [
		"fxwatch"
	],
	"author": "Evgeny Gridasov",
	"license": "BSD-Source-Code",
	"repository": {
		"type": "git",
		"url": "https://olinyavod.visualstudio.com/DefaultCollection/_git/TradingChart"
	},
	"dependencies": {
		"core-js": "^2.5.2",
		"devextreme": "^17.2.3",
		"flag-icon-css": "^2.9.0",
		"group-array": "^0.3.3",
		"immutable": "^3.8.2",
		"jquery": "^3.2.1",
		"promise-polyfill": "^6.1.0",
		"react": "^16.2.0",
		"react-dom": "^16.2.0",
		"react-redux": "^5.0.6",
		"redux": "^3.7.2",
		"redux-thunk": "^2.2.0",
		"whatwg-fetch": "^2.0.3"
	},
	"devDependencies": {
		"@types/core-js": "^0.9.43",
		"@types/react": "^16.0.28",
		"@types/react-dom": "^16.0.3",
		"@types/react-redux": "^5.0.14",
		"@types/jquery": "^3.2.16",
		"awesome-typescript-loader": "^3.4.1",
		"babel-cli": "^6.26.0",
		"babel-core": "^6.26.0",
		"babel-jest": "^21.2.0",
		"babel-loader": "^7.1.2",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-react": "^6.24.1",
		"babel-preset-stage-0": "^6.24.1",
